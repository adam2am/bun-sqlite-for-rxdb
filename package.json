{
  "name": "bun-sqlite-for-rxdb",
  "version": "1.3.1",
  "author": "adam2am",
  "repository": {
    "type": "git",
    "url": "https://github.com/adam2am/bun-sqlite-for-rxdb.git"
  },
  "main": "dist/index.js",
  "dependencies": {},
